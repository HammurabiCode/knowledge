    <li class="comment-item" data-cid="487424181">
        <div class="comment">
            <h3>
                <span class="comment-vote">
                    <span id="c-487424181" class="vote-count">0</span>
                        <a href="javascript:;" id="btn-487424181" class="j a_show_login" data-cid="487424181">有用</a>
