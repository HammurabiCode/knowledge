  $('.more-ebooks').on('click', function() {
    var $this = $(this),
      $li = $this.siblings('ul').find('li');
    if ($this.hasClass('isShow')) {
      $(this).text('å±•å¼€æ›´å¤š').removeClass('isShow');
      $li.not(':first').addClass('hide');
    }else{
      $(this).text('æ”¶èµ·').addClass('isShow');
      $li.removeClass('hide');
    }
    
  })
  </script>

